<table class='table'>
    <div class='spacer'></div>
    <div class='fixed'>
        <div class='title'>{{key}}</div>
    </div>
    <div class='tablerownoborder'>
        <div class='tableheader'>AIRSTRIP INFORMATION</div>
    </div>
    <div class='tablerow'>
        <div class='tablefield'>
            <p class='tablelabel'>Elevation</p>
        </div>
        <div class='tablefield'>
            <p class='tablevalue'>{{elevation}}'</p>
        </div>
    </div>
    <div class='tablerownoborder'>
        <div class='tablelabel'><a
                href=https://forecast.weather.gov/MapClick.php?lat={{lat}}&lon=-{{lon}}#.X8nNmS1Ha-o>Weather
                from National Weather Service</a></div>
    </div>
    <div class='tablerownoborder'>
        <div class='tablelabel'><a
                href=https://www.google.com/maps/@?api=1&map_action=map&center={{lat}},-{{lon}}&zoom=16&basemap=satellite>Google
                Maps</a></div>
    </div>
    <div class='tablerownoborder'>
        <div class='tableheader'>RUNWAY INFORMATION</div>
    </div>
    <div class='tablerow'>
        <div class='tablefield'>
            <p class='tablelabel'>Runway</p>
        </div>
        <div class='tablefield'>
            <p class='tablevalue'>{{runway['primary_runway']}}</p>
        </div>
    </div>
    <div class='tablerow'>
        <div class='tablefield'>
            <p class='tablelabel'>Length</p>
        </div>
        <div class='tablefield'>
            <p class='tablevalue'>{{runway['length']}}'</p>
        </div>
    </div>
    <div class='tablerow'>
        <div class='tablefield'>
            <p class='tablelabel'>Surface</p>
        </div>
        <div class='tablefield'>
            <p class='tablevalue'>{{runway['type']}}</p>
        </div>
    </div>
    <div class='tablerownoborder'>
        <div class='tableheader'>LAND INFORMATION</div>
    </div>
    {% for manager in managers %}
    <div class='tablerownoborder'>
        <div class='tablefield'>
            <p class='tablelabel'>Manager</p>
        </div>
        <div class='tablefield'>
            <p class='tablevalue'>{{manager}}</p>
        </div>
    </div>
    {% endfor %}
    <div class='tablerownoborder'>
        <div class='tableheader'>DESCRIPTION</div>
    </div>
    <div class='tablerownoborder'>
        <div class='extraspaceafter'>
            <p class='text'>
            <p>{description}</p>
            </p>
        </div>
    </div>

    <p class='text websitereference'>Airport Reference <a
            href='{{link}}}'>{{key}}</a>.</p>
    <p class='borderbottom'>
        </div>]]></description>